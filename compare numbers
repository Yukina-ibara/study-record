package hello;

import java.util.Scanner;

public class hello{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in );
		int x;
		int y;
		System.out.println("请输入两个数：");
		x = in.nextInt();
		y = in.nextInt();
		int max = 0;
		if (x>y)
		{
			max = x;
		}
		else max = y;
		System.out.println(max);
	}

}
