package hello;

import java.util.Scanner;

public class hello{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in );
		int num = in.nextInt();
		int x = 1;
		int y = 0;
		for(;num>=x;x*=2) {
			y++;
		}
		System.out.print(32-y);
	}
}
