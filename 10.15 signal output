package hello;

import java.util.Scanner;

public class hello{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in );
		int signal = in.nextInt();
		switch(signal%10)
		{
		case 1:
			System.out.print("Faint signals, barely perceptible, ");
			break;
		case 2:
			System.out.print("Very weak signals, ");
			break;
		case 3:
			System.out.print("Weak signals, ");
			break;
		case 4:
			System.out.print("Fair signals, ");
			break;
		case 5:
			System.out.print("Fairly good signals, ");
			break;
		case 6:
			System.out.print("Good signals, ");
			break;
		case 7:
			System.out.print("Moderately strong signals, ");
			break;
		case 8:
			System.out.print("Strong signals, ");
			break;
		case 9:
			System.out.print("Extremely strong signals, ");
			break;		
		}
		if (11<=signal==signal<=19)
		{
			System.out.println("unreadable.");
		}
		else if(20<=signal==signal<=29)
		{
			System.out.println("barely readable, occasional words distinguishable.");
		}
		else if(30<=signal==signal<=39)
		{
			System.out.println("readable with considerable difficulty.");

		}
		else if(40<=signal==signal<=49)
		{
			System.out.println("readable with practically no difficulty.");

		}
		else 
			System.out.println("perfectly readable.");
	}

}
